{
    "blocks": {
        "core/audio": {
            "css": "& { display: block; }"
        },
        "core/button": {
            "variations": {
                "outline": {
                    "border": {
                        "color": "var(--wp--preset--color--theme-4)",
                        "width": "1px"
                    },
                    "spacing": {
                        "padding": {
                            "bottom": "5px",
                            "left": "calc(var(--wp--preset--spacing--20) - 1px)",
                            "right": "calc(var(--wp--preset--spacing--20) - 1px)",
                            "top": "5px"
                        }
                    }
                }
            }
        },
        "core/buttons": {
            "spacing": {
                "blockGap": "var(--wp--preset--spacing--10)",
                "margin": {
                    "bottom": "var(--wp--preset--spacing--30)",
                    "top": "var(--wp--preset--spacing--30)"
                }
            }
        },
        "core/code": {
            "color": {
                "background": "var(--wp--preset--color--theme-1)"
            },
            "css": "& code { overflow: scroll; white-space: pre !important; }",
            "border": {
                "radius": "var(--wp--custom--border--radius--default)",
                "width": "1px",
                "style": "solid",
                "color": "var(--wp--preset--color--theme-4)"
            },
            "spacing": {
                "margin": {
                    "bottom": "var(--wp--preset--spacing--30)",
                    "top": "var(--wp--preset--spacing--30)"
                },
                "padding": {
                    "bottom": "var(--wp--preset--spacing--20)",
                    "left": "var(--wp--preset--spacing--20)",
                    "right": "var(--wp--preset--spacing--20)",
                    "top": "var(--wp--preset--spacing--20)"
                }
            },
            "typography": {
                "fontFamily": "var(--wp--preset--font-family--geist-mono)",
                "fontSize": "80%"
            }
        },
        "core/comment-author-name": {
            "typography": {
                "fontWeight": "550"
            }
        },
        "core/comment-content": {
            "css": ".comment-awaiting-moderation { color: var(--wp--preset--color--theme-5); font-size: 90%; }"
        },
        "core/comment-date": {
            "color": {
                "text": "var(--wp--preset--color--theme-5)"
            },
            "elements": {
                "link": {
                    ":hover": {
                        "color": {
                            "text": "var(--wp--preset--color--theme-6)"
                        }
                    },
                    "color": {
                        "text": "var(--wp--preset--color--theme-5)"
                    },
                    "typography": {
                        "textDecoration": "none"
                    }
                }
            },
            "typography": {
                "fontSize": "90%"
            }
        },
        "core/comment-edit-link": {
            "elements": {
                "link": {
                    ":hover": {
                        "color": {
                            "text": "var(--wp--preset--color--theme-6)"
                        }
                    },
                    "color": {
                        "text": "var(--wp--preset--color--theme-5)"
                    },
                    "typography": {
                        "textDecoration": "none"
                    }
                }
            },
            "typography": {
                "fontSize": "90%"
            }
        },
        "core/comment-reply-link": {
            "css": "& a { transition: background-color var(--wp--custom--transition--duration) ease-out, color var(--wp--custom--transition--duration) ease-out, shadow var(--wp--custom--transition--duration) ease-out; }",
            "elements": {
                "link": {
                    ":hover": {
                        "color": {
                            "background": "var(--wp--preset--color--theme-2)"
                        },
                        "shadow": "var(--wp--preset--shadow--inset-2)"
                    },
                    "border": {
                        "radius": "var(--wp--custom--border--radius--button)"
                    },
                    "color": {
                        "text": "var(--wp--preset--color--theme-6)"
                    },
                    "shadow": "var(--wp--preset--shadow--inset)",
                    "spacing": {
                        "padding": {
                            "bottom": "7px",
                            "left": "10px",
                            "right": "10px",
                            "top": "7px"
                        }
                    },
                    "typography": {
                        "fontSize": "90%",
                        "lineHeight": "1",
                        "textDecoration": "none"
                    }
                }
            },
            "spacing": {
                "margin": {
                    "top": "calc( var(--wp--preset--spacing--30) - 4px) !important"
                }
            }
        },
        "core/comment-template": {
            "css": "& ol { padding-left: calc( var(--wp--preset--spacing--40) + 12px) }"
        },
        "core/comments-title": {
            "typography": {
                "fontSize": "var(--wp--preset--font-size--x-large)"
            }
        },
        "core/details": {
            "border": {
                "radius": "var(--wp--custom--border--radius--button)"
            },
            "color": {
                "background": "var(--wp--preset--color--theme-2)"
            },
            "css": "& summary { font-weight: 550; } & > p { margin-top: var(--wp--preset--spacing--10); }",
            "spacing": {
                "padding": {
                    "bottom": "calc( var(--wp--preset--spacing--20) * 0.75)",
                    "left": "var(--wp--preset--spacing--20)",
                    "right": "var(--wp--preset--spacing--20)",
                    "top": "calc( var(--wp--preset--spacing--20) * 0.75)"
                }
            }
        },
        "core/file": {
            "css": "& :where(.wp-block-file__button):is(a):hover { opacity: 1} & *+.wp-block-file__button {margin-left: var(--wp--preset--spacing--20)} &:not(.wp-element-button) { font-size: var(--wp--preset--font-size--medium) }"
        },
        "core/gallery": {
            "css": "& { --wp--style--gallery-gap-default: calc( var(--wp--preset--spacing--10) / 2 ) }",
            "spacing": {
                "margin": {
                    "bottom": "var(--wp--preset--spacing--30)",
                    "top": "var(--wp--preset--spacing--30)"
                }
            }
        },
        "core/image": {
            "border": {
                "radius": "var(--wp--custom--border--radius--default)"
            },
            "css": "& .lightbox-trigger { visibility: hidden; }",
            "spacing": {
                "margin": {
                    "bottom": "var(--wp--preset--spacing--30)",
                    "top": "var(--wp--preset--spacing--30)"
                }
            }
        },
        "core/navigation": {
            "color": {
                "text": "var(--wp--preset--color--theme-5)"
            },
            "css": "&.is-vertical a { padding-left: 0; padding-right: 0; } & .current-menu-item > a { color: var(--wp--preset--color--theme-6) !important; }",
            "elements": {
                "link": {
                    ":hover": {
                        "color": {
                            "text": "var(--wp--preset--color--theme-6) !important"
                        }
                    },
                    "border": {
                        "radius": "--wp--custom--border--radius--button"
                    },
                    "color": {
                        "text": "var(--wp--preset--color--theme-6)"
                    },
                    "typography": {
                        "lineHeight": "1",
                        "textDecoration": "none"
                    }
                }
            },
            "spacing": {
                "blockGap": "var(--wp--preset--spacing--20)"
            },
            "typography": {
                "lineHeight": "1"
            }
        },
        "core/paragraph": {
            "css": "&:empty { display: none; }"
        },
        "core/post-comments-form": {
            "css": "& textarea { max-height: 128px;} & .form-submit { margin-top: calc( var(--wp--preset--spacing--20) + 4px) }"
        },
        "core/post-comments-link": {
            "color": {
                "text": "var(--wp--preset--color--theme-5)"
            },
            "elements": {
                "link": {
                    ":hover": {
                        "color": {
                            "text": "var(--wp--preset--color--theme-6)"
                        },
                        "typography": {
                            "textDecoration": "none"
                        }
                    },
                    "color": {
                        "text": "var(--wp--preset--color--theme-5)"
                    },
                    "typography": {
                        "textDecoration": "none"
                    }
                }
            },
            "typography": {
                "fontSize": "var(--wp--preset--font-size--small)"
            }
        },
        "core/post-date": {
            "color": {
                "text": "var(--wp--preset--color--theme-5)"
            },
            "elements": {
                "link": {
                    ":hover": {
                        "color": {
                            "text": "var(--wp--preset--color--theme-6)"
                        },
                        "typography": {
                            "textDecoration": "none"
                        }
                    },
                    "color": {
                        "text": "var(--wp--preset--color--theme-5)"
                    },
                    "typography": {
                        "textDecoration": "none"
                    }
                }
            },
            "typography": {
                "fontSize": "var(--wp--preset--font-size--small)"
            }
        },
        "core/post-excerpt": {
            "css": "& p { margin-top: var(--wp--preset--spacing--10) }"
        },
        "core/post-title": {
            "color": {
                "text": "var(--wp--preset--color--theme-6)"
            },
            "typography": {
                "fontSize": "var(--wp--preset--font-size--xx-large)"
            },
            "elements": {
                "link": {
                    ":hover": {
                        "color": {
                            "text": "var(--wp--preset--color--theme-5)"
                        }
                    },
                    "color": {
                        "text": "currentColor"
                    },
                    "typography": {
                        "textDecoration": "none"
                    }
                }
            }
        },
        "core/preformatted": {
            "typography": {
                "fontFamily": "monospace",
                "fontSize": "90%"
            }
        },
        "core/pullquote": {
            "border": {
                "radius": "var(--wp--custom--border--radius--default)"
            },
            "color": {
                "background": "var(--wp--preset--color--theme-2)"
            },
            "css": "& cite { font-size: 90%; color: var(--wp--preset--color--theme-5); font-style: normal;}",
            "spacing": {
                "margin": {
                    "bottom": "var(--wp--preset--spacing--30)",
                    "top": "var(--wp--preset--spacing--30)"
                },
                "padding": {
                    "bottom": "var(--wp--preset--spacing--40)",
                    "left": "var(--wp--preset--spacing--40)",
                    "right": "var(--wp--preset--spacing--40)",
                    "top": "var(--wp--preset--spacing--40)"
                }
            },
            "typography": {
                "fontSize": "var(--wp--preset--font-size--large)"
            }
        },
        "core/query-pagination": {
            "elements": {
                "link": {
                    ":hover": {
                        "color": {
                            "text": "var(--wp--preset--color--theme-6)"
                        }
                    },
                    "color": {
                        "text": "var(--wp--preset--color--theme-5)"
                    },
                    "typography": {
                        "textDecoration": "none"
                    }
                }
            }
        },
        "core/query-pagination-numbers": {
            "css": "& { display: flex; gap: 2px; } & .page-numbers { padding: 8px; }"
        },
        "core/quote": {
            "border": {
                "color": "var(--wp--preset--color--theme-4)",
                "style": "solid",
                "width": "0 0 0 1px"
            },
            "css": "& p { opacity: 0.85 }",
            "spacing": {
                "margin": {
                    "bottom": "var(--wp--preset--spacing--30)",
                    "left": "0",
                    "right": "0",
                    "top": "var(--wp--preset--spacing--30)"
                },
                "padding": {
                    "left": "var(--wp--preset--spacing--20)"
                }
            },
            "typography": {
                "fontStyle": "italic",
                "lineHeight": "1.5"
            },
            "variations": {
                "plain": {
                    "border": {
                        "width": "0"
                    },
                    "spacing": {
                        "padding": {
                            "left": "0"
                        }
                    },
                    "typography": {
                        "fontStyle": "normal"
                    }
                }
            }
        },
        "core/separator": {
            "border": {
                "top": {
                    "width": "1px"
                }
            },
            "color": {
                "text": "var(--wp--preset--color--theme-3)"
            },
            "spacing": {
                "margin": {
                    "bottom": "var(--wp--preset--spacing--50)",
                    "top": "var(--wp--preset--spacing--50)"
                }
            }
        },
        "core/site-title": {
            "elements": {
                "link": {
                    "typography": {
                        "textDecoration": "none"
                    }
                }
            },
            "typography": {
                "fontSize": "var(--wp--preset--font-size--large)"
            }
        },
        "core/table-of-contents": {
            "border": {
                "radius": "var(--wp--custom--border--radius--default)"
            },
            "color": {
                "background": "var(--wp--preset--color--theme-3)"
            },
            "css": "& { transition: all var(--wp--custom--transition--duration) ease-out; }",
            "spacing": {
                "margin": {
                    "bottom": "var(--wp--preset--spacing--30)",
                    "top": "var(--wp--preset--spacing--30)"
                },
                "padding": {
                    "bottom": "var(--wp--preset--spacing--30)",
                    "left": "var(--wp--preset--spacing--30)",
                    "right": "var(--wp--preset--spacing--30)",
                    "top": "var(--wp--preset--spacing--30)"
                }
            }
        },
        "core/tag-cloud": {
            "elements": {
                "link": {
                    ":hover": {
                        "color": {
                            "background": "var(--wp--preset--color--theme-1)",
                            "text": "var(--wp--preset--color--theme-6)"
                        }
                    },
                    "color": {
                        "background": "var(--wp--preset--color--theme-1)",
                        "text": "var(--wp--preset--color--theme-5)"
                    },
                    "spacing": {
                        "padding": {
                            "bottom": "var(--wp--preset--spacing--10)",
                            "left": "calc(var(--wp--preset--spacing--10) * 1.5)",
                            "right": "calc(var(--wp--preset--spacing--10) * 1.5)",
                            "top": "var(--wp--preset--spacing--10)"
                        }
                    },
                    "typography": {
                        "textDecoration": "none",
                        "lineHeight": "1.2"
                    }
                }
            },
            "spacing": {
                "margin": {
                    "bottom": "0",
                    "right": "0"
                }
            },
            "typography": {
                "fontSize": "var(--wp--preset--font-size--small) !important"
            }
        },
        "core/video": {
            "css": "& video { border-radius: var(--wp--custom--border--radius--default); }",
            "spacing": {
                "margin": {
                    "bottom": "var(--wp--preset--spacing--30)",
                    "top": "var(--wp--preset--spacing--30)"
                }
            }
        }
    }
} 